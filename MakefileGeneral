RM := rm -rf
TARGET := program
OBJS := subprojects/LibToTree23/submodules/BTree/nodeOfBTree.o subprojects/LibToTree23/submodules/BTree/btree.o tree23.o program.o
SRCS := subprojects/LibToTree23/submodules/BTree/nodeOfBTree.cpp subprojects/LibToTree23/submodules/BTree/btree.cpp tree23.cpp program.cpp

all: $(TARGET)

$(TARGET): $(OBJS) $(SRCS)
	@echo 'Building ' $(TARGET)
	@g++ -Isubprojects/LibToTree23/submodules/BTree/ -o $(TARGET) $(OBJS)
	@echo 'End of build'
	
%.o: %.cpp
	@echo 'Building $@ from $<'
	@g++ -Isubprojects/LibToTree23/submodules/BTree/ -o $@ -c $<
	
clean:
	@echo "cleaning"
	$(RM) $(OBJS) $(TARGET) subprojects/LibToTree23/submodules/BTree/*.o subprojects/LibToTree23/submodules/BTree/$(TARGET)
